include grids.mini

[__local]
wdim = 2, 3 | expand world_dim

[__local.cg]
fem_grids = AluCube{__local.wdim}dLeafGridPartType, AluConform{__local.wdim}dLeafGridPartType, Yasp{__local.wdim}dLeafGridPartType, Yasp1dLeafGridPartType | expand cg_fem_grids
pdelab_grids = AluCube{__local.wdim}dLeafGridViewType, AluConform{__local.wdim}dLeafGridViewType, Yasp{__local.wdim}dLeafGridViewType, Yasp1dLeafGridViewType | expand cg_pdelab_grids
fem = Dune::GDT::DuneFemCgSpaceWrapper<{__local.cg.fem_grids}, 1, double, 1>
pdelab =  Dune::GDT::DunePdelabCgSpaceWrapper<{__local.cg.pdelab_grids}, 1, double, 1>

names = cg_fem, cg_pdelab | expand cg_backends
grid_name = {grids.names.{__local.cg.fem_grids}}, {grids.names.{__local.cg.pdelab_grids}} | expand cg_backends
spaces = {__local.cg.fem}, {__local.cg.pdelab} | expand cg_backends

[__local.dg]
fem_grids = AluCube{__local.wdim}dLeafGridPartType, AluConform{__local.wdim}dLeafGridPartType, Yasp{__local.wdim}dLeafGridPartType, Yasp1dLeafGridPartType | expand dg_fem_grids
pdelab_grids = AluCube{__local.wdim}dLeafGridViewType, Yasp{__local.wdim}dLeafGridViewType, Yasp1dLeafGridViewType | expand dg_pdelab_grids
fem = Dune::GDT::DuneFemDgSpaceWrapper<{__local.dg.fem_grids}, 1, double, 1>
pdelab =  Dune::GDT::DunePdelabDgSpaceWrapper<{__local.dg.pdelab_grids}, 1, double, 1>

names = dg_fem, dg_pdelab | expand dg_backends
grid_name = {grids.names.{__local.dg.fem_grids}}, {grids.names.{__local.dg.pdelab_grids}} | expand dg_backends
spaces = {__local.dg.fem}, {__local.dg.pdelab} | expand dg_backends

[__local.fv]
rdim = 1, 2, 3 | expand fv_rdim
grids =  AluCube{__local.wdim}dLeafGridViewType, AluConform{__local.wdim}dLeafGridViewType, Yasp{__local.wdim}dLeafGridViewType, Yasp1dLeafGridViewType | expand fv_grids
grid_name = {grids.names.{__local.fv.grids}}
names = fv_default_R{__local.fv.rdim}
spaces = Dune::GDT::FvSpace<{__local.fv.grids}, double, {__local.fv.rdim}>

[__local.rt]
wdim = 2, 3 | expand rt_rdim
grids =  AluCube{__local.rt.wdim}dLeafGridViewType, AluConform{__local.rt.wdim}dLeafGridViewType, Yasp{__local.rt.wdim}dLeafGridViewType | expand rt_grids
grid_name = {grids.names.{__local.rt.grids}}
names = rt_pdelab_R{__local.rt.rdim}
spaces = Dune::GDT::DunePdelabRtSpaceWrapper<{__local.rt.grids}, 0, double, {__local.rt.rdim}>

